#include <iostream>

using namespace std;

void exibirVetor(int *vetor, int tamanho) {
    cout << "Elementos do vetor: ";
    for (int i = 0; i < tamanho; i++) {
        cout << vetor[i] << " ";
    }
    cout << endl;
}

int main() {
    const int TAM = 5;
    int vetor[TAM];

    cout << "Digite 5 valores inteiros para o vetor:" << endl;
    for (int i = 0; i < TAM; i++) {
        cin >> vetor[i];
    }

    exibirVetor(vetor, TAM);

    return 0;
}
